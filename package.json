		"vite-tsconfig-paths": "^4.0.5"
	},
	"dependencies": {
		"blakejs": "^1.2.1",
		"lodash": "^4.17.21",
		"papaparse": "^5.3.2",
		"reflect-metadata": "^0.1.13",
		"syrinscape": "file:libs/syrinscape",
		"tsyringe": "^4.7.0"
	}
